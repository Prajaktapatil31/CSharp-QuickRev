using System;

class Program
{
    static void Main()
    {
        int age = 20;
        double pi = 3.14;
        char grade = 'A';
        string name = "Prajakta";
        bool isStudent = true;

        Console.WriteLine($"{name}, Age: {age}, Grade: {grade}, Pi: {pi}, Student: {isStudent}");
    }
}


Output:

Prajakta, Age: 20, Grade: A, Pi: 3.14, Student: True
